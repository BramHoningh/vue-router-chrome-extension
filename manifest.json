{
  "manifest_version": 2,
  "name": "Vue Router Navigator",
  "description": "Easy navigate your vue app",
  "author": "Pixeldenker",
  "version": "0.1.0",
  "permissions": ["activeTab", "tabs"],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "scripts": ["scripts/background.js"]
  }
}
